              write.rc \
              write.c

UMTYPE=       windows
UMENTRY=      wwinmain

# Fusionized
SXS_MANIFEST_RESOURCE_ID=1
SXS_APPLICATION_MANIFEST=write.manifest
SXS_ASSEMBLY_NAME=Microsoft.Windows.Shell.write
SXS_NO_BINPLACE=1
SXS_MANIFEST_IN_RESOURCES=1

