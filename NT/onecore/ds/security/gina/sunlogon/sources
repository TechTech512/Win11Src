MAJORCOMP=windows
MINORCOMP=winlogon

TARGETNAME=sunlogon
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=$(WINDOWS_INC_PATH);$(BASE_INC_PATH)
C_DEFINES=-DUNICODE -D_UNICODE

SOURCES= \
         sunlogon.c \
         res.rc

USE_MSVCRT=1

UMTYPE=windows
UMENTRY=winmain
LINKLIBS= \
        $(WINDOWS_LIB_PATH)\user32p.lib \
        $(WINDOWS_LIB_PATH)\gdi32p.lib \
        $(SDK_LIB_PATH)\userenv.lib \
        $(SDK_LIB_PATH)\ntdll.lib
