IDR_CPU_CONTEXT MENUEX
BEGIN
    POPUP "CPU Context", 0, MFT_STRING, MFS_ENABLED, 0
	BEGIN
	    MENUITEM "&End task", ID_END_TASK, MFT_STRING, MFS_ENABLED
